generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int        @id
  username    String
  status      Int
  displayName String
  ipAddress   String
  timezone    String
  lastVersion String
  changePw    Boolean
  userRoles   UserRole[]
}

model UserRole {
  // userId Int @id
  // roleId Int
  User   User? @relation(fields: [userId], references: [id])
  userId Int?
}

model Role {
  id   Int    @id
  name String
}
